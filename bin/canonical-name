#!/usr/bin/env python

from __future__ import print_function
import os
import sys

def print_error(msg):
    print(msg, file=sys.stderr)

if len(sys.argv) != 2:
    print_error("Error: single argument required")
    sys.exit()
    
filename = sys.argv[1]

norm_path = os.path.realpath(filename)

print(norm_path)
